{% extends 'layouts/base.twig' %}
{% block content %}
    <div class="page_content">
 
        <img src="{{ post.thumbnail.src }}" />
 
    </div>
    <div class="blog-post">
        <h2 class="blog-post-title mb-3">{{ post.title }}</h2>
        {{ post.content | shortcodes }}
    </div>
{% endblock %}